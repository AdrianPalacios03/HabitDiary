{
  "expo": {
    "name": "HabitDiary",
    "slug": "HabitDiary",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#9568e3"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.palacios.habitdiary"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#9568e3"
      },
      "package": "com.palacios.habitdiary"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1aabf7db-f9ca-483c-b030-367cbc7d1876"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/1aabf7db-f9ca-483c-b030-367cbc7d1876"
    }
  }
}
